// Solution as follows
#include <bits/stdc++.h>
using namespace std;

int main() 
{
 int t;
 cin>>t;
 while(t--)
  { 
   int A,B,D,Q;
   //Accept 2 integers as inputs
   cin>>A>>B;
   //Difference of inputs
   D = A - B;               
   //Division of inputs. 
   Q = A / B;      
   //Print the desired output for each test case
    cout<<D<<" "<<Q<<endl;  
   }
 return 0;
}
