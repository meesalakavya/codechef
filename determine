// Solution as follows

#include <bits/stdc++.h>
using namespace std;

int main() 
{
 int t;
 cin>>t;
 while(t--)
  { 
   int X,N;
   // The variables "points_per_testcase" and "score" were not declared.
   int points_per_testcase,score;
   // The inputs for X and N were not taken.
   cin>>X>>N;
   points_per_testcase=X/10;
   score= points_per_testcase*N;
   cout<<score<<endl;
   }
 return 0;
}
