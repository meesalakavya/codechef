// Solution as follows

#include <bits/stdc++.h>
using namespace std;

int main() 
{
 int X = 30;
 if (X%3 == 0)
    {
      cout<<"Number of questions incorrctly solved is 0"<<endl;
    }
 else if ((X+1)%3 == 0)
    {
      cout<<"Number of questions incorrctly solved is 1"<<endl;
    }
else
    {
      cout<<"Number of questions incorrctly solved is 2"<<endl;
    }
    
 X=34;
 if (X%3 == 0)
    {
      cout<<"Number of questions incorrctly solved is 0"<<endl;
    }
 else if ((X+1)%3 == 0)
    {
      cout<<"Number of questions incorrctly solved is 1"<<endl;
    }
 else
    {
      cout<<"Number of questions incorrctly solved is 2"<<endl;
    }

return 0;
}
